{
    "python.defaultInterpreterPath": "./.venv/Scripts/python.exe",
    "python.terminal.activateEnvironment": true,
    "terminal.integrated.cwd": "${workspaceFolder}",
    "terminal.integrated.profiles.windows": {
        "Milvus Python": {
            "path": "cmd.exe",
            "args": ["/k", ".venv\\Scripts\\activate.bat"]
        }
    },
    "terminal.integrated.defaultProfile.windows": "Milvus Python",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.retool_types/**": true,
        "**/*tsconfig.json": true,
        ".cache": true,
        "retool.config.json": true
    }
}